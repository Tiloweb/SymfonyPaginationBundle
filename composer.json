{
    "name": "tiloweb/pagination-bundle",
    "description": "A simple and elegant pagination bundle for Symfony using Doctrine ORM Paginator",
    "type": "symfony-bundle",
    "keywords": [
        "symfony",
        "bundle",
        "pagination",
        "paginator",
        "pager",
        "doctrine",
        "orm"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Thibault HENRY",
            "email": "thibault@henry.pro",
            "homepage": "https://tiloweb.com",
            "role": "Lead Developer"
        }
    ],
    "homepage": "https://github.com/Tiloweb/SymfonyPaginationBundle",
    "support": {
        "issues": "https://github.com/Tiloweb/SymfonyPaginationBundle/issues",
        "source": "https://github.com/Tiloweb/SymfonyPaginationBundle"
    },
    "require": {
        "php": ">=8.2",
        "doctrine/orm": "^2.14|^3.0",
        "symfony/framework-bundle": "^6.4|^7.0|^8.0",
        "symfony/http-foundation": "^6.4|^7.0|^8.0",
        "symfony/http-kernel": "^6.4|^7.0|^8.0",
        "symfony/dependency-injection": "^6.4|^7.0|^8.0",
        "symfony/config": "^6.4|^7.0|^8.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpunit/phpunit": "^11.0",
        "symfony/phpunit-bridge": "^6.4|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tiloweb\\PaginationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tiloweb\\PaginationBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "require": "^6.4|^7.0|^8.0"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
